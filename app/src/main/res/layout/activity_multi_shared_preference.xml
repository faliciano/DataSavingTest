<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MultiSharedPreferenceActivity">

    <Button
        android:id="@+id/button_read_multi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="读数据1"
        app:layout_constraintEnd_toStartOf="@+id/button_write_multi"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_write_multi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="24dp"
        android:text="写数据1"
        app:layout_constraintEnd_toStartOf="@+id/button_read_multi_2"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_write_multi_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="12dp"
        android:text="写数据2"
        app:layout_constraintBottom_toTopOf="@+id/textView_console_multi"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/button_read_multi_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="读数据2"
        app:layout_constraintBottom_toTopOf="@+id/textView_console_multi"
        app:layout_constraintEnd_toStartOf="@+id/button_write_multi_2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView_console_multi"
        android:layout_width="381dp"
        android:layout_height="265dp"
        android:text="无"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_write_multi" />

    <TextView
        android:id="@+id/textView_console_multi_2"
        android:layout_width="380dp"
        android:layout_height="322dp"
        android:layout_marginBottom="40dp"
        android:text="无"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>